{
    "title": "Dewey JSON",
    "copyright": "Copyright @ Refreshing Collective, LLC",
    "version": "6.0.0",
    "description": "A collection of .Net utilities for Json",
    "authors": [
        "Refreshing Collective <contact@refreshing.io>"
    ],
    "buildOptions": {
        "allowUnsafe": true,
        "debugType": "portable",
        "define": [ "DEBUG", "TRACE" ],
        "xmlDoc": true,
        "optimize": true
    },
    "packOptions": {
        "tags": [
            "Utilities",
            "Extensions"
        ],
        "iconUrl": "https://raw.githubusercontent.com/RefreshingIO/dewey/master/assets/dewey-32x32.png",
        "projectUrl": "https://github.com/RefreshingIO/dewey",
        "licenseUrl": "https://raw.githubusercontent.com/RefreshingIO/refreshing-ui/master/LICENSE",
        "owners": [
            "Refreshing Collective <contact@refreshing.io>"
        ],
        "repository": {
            "type": "git",
            "url": "git://github.com/RefreshingIO/dewey"
        },
        "summary": "A collection of .Net utilities for Json"
    },
    "dependencies": {
        "Newtonsoft.Json": "9.0.1"
    },
    "frameworks": {
        "net45": {
            "frameworkAssemblies": {
                "System.Net.Http": ""
            }
        },
        "net451": {
            "frameworkAssemblies": {
                "System.Net.Http": ""
            }
        },
        "net452": {
            "frameworkAssemblies": {
                "System.Net.Http": ""
            }
        },
        "net46": {
            "frameworkAssemblies": {
                "System.Net.Http": ""
            }
        },
        "net461": {
            "frameworkAssemblies": {
                "System.Net.Http": ""
            }
        },
        "dnx451": {
            "frameworkAssemblies": {
                "System.Net.Http": ""
            }
        },
        "dnx46": {
            "frameworkAssemblies": {
                "System.Net.Http": ""
            }
        },
        "netcoreapp1.0": {
            "imports": [
                "dotnet5.6",
                "dnxcore50",
                "portable-net45+win8"
            ],
            "dependencies": {
                "Microsoft.NETCore.App": "1.0.1",
                "NETStandard.Library": "1.6.0"
            }
        },
        "netstandard1.3": {
            "imports": [
                "dotnet5.6",
                "dnxcore50",
                "portable-net45+win8"
            ],
